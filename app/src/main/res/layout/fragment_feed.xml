<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/molang2">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Feedback"
        android:textSize="30dp"
        android:textStyle="bold"
        android:id="@+id/textView5"
        android:layout_gravity="center_horizontal"
        android:layout_x="104dp"
        android:layout_y="35dp" />

    <MultiAutoCompleteTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="New MultiAutoCompleteTextView"
        android:textColorHint="#FFB9B7B7"
        android:id="@+id/multiAutoCompleteTextView"
        android:layout_x="1dp"
        android:layout_y="224dp" />

    <Button
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:text="Send Feedback"
        android:id="@+id/button3"
        android:layout_gravity="center_horizontal"
        android:layout_x="114dp"
        android:layout_y="310dp" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/hola"
        android:layout_x="245dp"
        android:layout_y="41dp"
        android:src="@drawable/log"/>


</AbsoluteLayout>